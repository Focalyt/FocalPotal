                        <div class="project-description" contenteditable="true" data-placeholder="Project Description"></div>
                    </div>
                `);
                container.insertBefore(newItem, container.querySelector('.add-UserBtn'));
            }
            
            function addInterest() {
                const container = document.querySelector('.interests-list');
                const newItem = createEditableElement(`
                    <div class="interest-tag" contenteditable="true" data-placeholder="Interest"></div>
                `);
                container.insertBefore(newItem, container.querySelector('.add-UserBtn'));
            }
            </script>
            
            
